<html>
<head>
  <meta http-equiv="content-type" content="text/html;charset=ISO-8859-1">
  <title><%= @title %></title>
  <link rel='stylesheet' type='text/css' href='styles/style.css'>
</head>
<body bgcolor="#FFFFFF" leftmargin="0" marginwidth="0" topmargin="0" marginheight="0">
<div align="center">
  <table width="767" cellpadding="0" cellspacing="0">
    <tr>
      <td colspan="5" align="center"><a href="index-2.html">
        <img src="assets/logo.gif" alt="Bay Area Mountain Rescue" title="BAMRU Home" width="216" height="51" border="0"></a><br>
        <img src="assets/graydot.gif" height="1" width="758" border="0"></td>
    </tr>
    <tr>
      <td width="172" valign=top rowspan="3"><a id="home"></a>
        <table cellpadding="6">
          <tr>
            <td align="right" NOWRAP>
              <p><font class="capsred">SERVING THE GREATER<br>
                &nbsp;&nbsp;SAN FRANCISCO BAY AREA<br>
              </font>
                <%= dot_hr %>
                <%= menu_link "/calendar.test",          "CALENDAR"           %>
                <%= menu_link blog_url,                  "BLOG"               %>
                <%= dot_hr %>
                <%= menu_link "/index-2.html",           "HOME"               %>
                <%= menu_link "/bamruinfo.test",         "WHO WE ARE"         %>
                <%= menu_link "/join.test",              "JOINING BAMRU"      %>
                <%= menu_link "/sgallery.test",          "PHOTO GALLERY"      %>
                <%= menu_link "/meeting_locations.test", "MEETING LOCATIONS"  %>
                <%= menu_link "/sarlinks.test",          "LINKS TO SAR SITES" %>
                <%= dot_hr %>
                <%= menu_link "/donate.test",            "DONATE"             %>
                <%= menu_link "/contact.test",           "CONTACT US"         %>
                <%= dot_hr %>
                <br>
                <br>
                <br>
                <br>
                <%= @left_txt %>
              </p>
              <br>
            </td>
          </tr>
        </table>
      </td>
      <td valign="top" rowspan="3"><img src="assets/graydot.gif" height="310" width="1" border="0"></td>
      <td valign="top"><img src="<%= @hdr_img %>" width="423" height="136" border="0"></td>
      <td valign="top" rowspan="3"><img src="assets/graydot.gif" height="310" width="1" border="0"></td>
      <td width="172" valign=top rowspan="3">
        <table cellpadding="6">
          <tr>
            <td NOWRAP>
              <%= @right_nav %>
              <img src="assets/dots.gif" width="134" height="10" border="0"><br>
              <p/>
              <%= @right_txt %>
            </td>
          </tr>
        </table>
      </td>
    </tr>
    <tr>
      <td><img src="assets/graydot.gif" height="1" width="423" border="0"></td>
    </tr>
    <tr>
      <td valign="top" height="380">


        <%= yield %>

      </td>
    </tr>
  </table>
  <br>
  <font class="copy">&copy; Copyright 2004-<%= Time.now.year %> Bay Area Mountain Rescue. &nbsp</font><br>
  <br><br>
</div>
</body>
</html>
