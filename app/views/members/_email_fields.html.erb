<li class=fields style="list-style-type: none; <%= check_style(@xemail) %>" id=email_<%=f.id%>>
  <span class=sort_handle><img src="/images/handle.png" class=handle></span>
  <%= f.hidden_field :position, :size => 1 %>
  <%= f.text_field :address, :size => 22 %>
  <%= f.select :typ, f.object.typ_opts %>
  Pgr<%= f.check_box :pagable %>
  <%= link_to_remove_field("remove", f) %>
  <%= f.hidden_field :_destroy %>
</li>