<div id=x_container>
  <div id=x_body>
    <div id=x_header>
      <table width=100%>
        <tr>
          <td align=left><b>DO Handoffs</b></td>
          <td align=right>
            <%= link_to("DO Schedule", do_assignments_path(@quarter)) %>
          </td>
        </tr>
      </table>
    </div>
    <div id=x_single_col class=centered>

      <table class=tablesorter>
        <tr>
          <th>Outgoing DO</th>
          <th>Incoming DO</th>
          <th>Status</th>
          <th>Handoff Date</th>
          <th>Action</th>
        </tr>
        <% @handoffs.each do |handoff| %>
          <tr>
            <td><%= handoff.outgoing_do.short_name %></td>
            <td><%= handoff.incoming_do.short_name %></td>
            <td><%= handoff.status %></td>
            <td><%= handoff.finished_at %></td>
            <td id=action_cell>NA</td>
          </tr>
        <% end %>
      </table>

    </div>
    <div id=x_footer>
      <%= link_to "Edit DO Assignments", edit_do_assignment_path(1, @quarter) %>
    </div>
  </div>
</div>