<script type="text/javascript">
  function usecaution() {
    alert("These CSV and HTM data elements are embedded in various documents.  Edit with care!!  You could break the system if you screw them up!!")
  };
  function showHandler() {
    $(".dElements").show();
    $("#dTog").removeClass("dShow");
    $("#dTog").addClass("dHide");
    $("#dTog").text("hide");
    $(".dHide").one('click', function(event){hideHandler(); return false;} );
  };
  function hideHandler() {
    $(".dElements").hide();
    $("#dTog").removeClass("dHide");
    $("#dTog").addClass("dShow");
    $("#dTog").text("show");
    $(".dShow").one('click', function(event){showHandler(); return false;} );
  };
  $(function() {
    $("#caution").click(function () {usecaution(); return false;});
    $(".dShow").one('click', function(event){showHandler(); return false;} );
    $(".dHide").one('click', function(event){hideHandler(); return false;} );
    hideHandler();
  });
</script>

